{
  "name": "vscode-luogu",
  "displayName": "vscode-luogu",
  "description": "Solve Luogu Problems in VSCode",
  "icon": "resources/img/favicon.ico",
  "version": "0.0.1",
  "license": "MIT",
  "publisher": "himself65",
  "engines": {
    "vscode": "^1.25.0"
  },
  "homepage": "https://github.com/Himself65/vscode-luogu/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/himself65/vscode-luogu.git"
  },
  "bugs": {
    "url": "https://github.com/himself65/vscode-luogu/issues",
    "email": "himself6565@gmail.com"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "keywords": [
    "luogu",
    "algorithm",
    "interview"
  ],
  "preview": true,
  "activationEvents": [
    "onCommand:luogu.about",
    "onCommand:luogu.signin",
    "onCommand:luogu.signout",
    "onCommand:luogu.showProblem",
    "onCommand:luogu.searchProblem",
    "onCommand:luogu.submitSolution",
    "onCommand:luogu.refreshExplorer",
    "onView:luoguExplorer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "luogu.signin",
        "title": "Sign in Luogu",
        "category": "Luogu",
        "icon": "resources/signin.svg"
      },
      {
        "command": "luogu.signout",
        "title": "Sign out Luogu",
        "category": "Luogu"
      },
      {
        "command": "luogu.showProblem",
        "title": "Show problem",
        "category": "Luogu"
      },
      {
        "command": "luogu.submitSolution",
        "title": "Submit to Luogu",
        "category": "Luogu"
      },
      {
        "command": "luogu.searchProblem",
        "title": "Search Problem",
        "category": "Luogu",
        "icon": "resources/search.svg"
      },
      {
        "command": "luogu.refreshExplorer",
        "title": "Refresh Explorer",
        "category": "Luogu",
        "icon": "resources/refresh.svg"
      },
      {
        "command": "luogu.about",
        "title": "About",
        "category": "Luogu"
      },
      {
        "command": "luogu.selectLanguage",
        "title": "SelectLanguage",
        "category": "Luogu"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "luogu",
          "title": "Luogu",
          "icon": "resources/img/favicon.ico"
        }
      ]
    },
    "views": {
      "luogu": [
        {
          "id": "luoguExplorer",
          "name": "Problems"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "luogu.signin",
          "when": "view == luoguExplorer",
          "group": "navigation@0"
        },
        {
          "command": "luogu.searchProblem",
          "when": "view == luoguExplorer",
          "group": "navigation@1",
          "icon": "resources/search.svg"
        },
        {
          "command": "luogu.refreshExplorer",
          "when": "view == luoguExplorer",
          "group": "navigation@2"
        }
      ],
      "view/item/context": [
        {
          "command": "luogu.showProblem",
          "when": "view == luoguExplorer && viewItem == problem",
          "group": "luogu@1"
        }
      ],
      "explorer/context": [
        {
          "command": "luogu.submitSolution",
          "when": "explorerResourceIsFolder == false",
          "group": "luogu@1"
        }
      ],
      "editor/context": [
        {
          "command": "luogu.submitSolution",
          "group": "luogu@1"
        }
      ]
    },
    "configuration": [
      {
        "title": "Luogu",
        "properties": {
          "luogu.defaultLanguage": {
            "type": "string",
            "enum": [
              "Auto",
              "C++11",
              "C++",
              "C",
              "Pascal",
              "Node.js",
              "Java",
              "Python2",
              "Python3"
            ],
            "default": "Auto",
            "scope": "window",
            "description": "Default language for solving the problems."
          },
          "luogu.showSelectLanguageHint": {
            "type": "boolean",
            "default": "true",
            "scope": "window",
            "description": "Show Select Language Hint When Submit"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "lint": "tslint --project tsconfig.json -e src/*.d.ts -t verbose"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "@luogu-dev/markdown-it-katex": "0.0.1",
    "@types/markdown-it": "0.0.4",
    "axios": "^0.18.0",
    "fs-extra": "^7.0.0",
    "markdown-it": "^8.4.2",
    "markdown-it-highlightjs": "^3.0.0"
  }
}
