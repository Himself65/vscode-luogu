{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "outDir": "out",
    "lib": [
      "es2017",
      "es2018",
      "dom",
      "dom.iterable"
    ],
    "sourceMap": true,
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "removeComments": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false
  },
  "exclude": [
    "node_modules",
    ".vscode-test",
    "CHANGELOG.md",
    ".travis.yml",
    ".gitignore",
    ".gitattributes",
    "CHANGELOG.md"
  ]
}
